{
  "name": "@angular-package/core",
  "version": "1.1.2",
  "description": "Core features",
  "author": "Angular Package <angular-package@wwwdev.io> (http://angular-package.wwwwdev.io)",
  "homepage": "https://github.com/angular-package/common#readme",
  "main": "dist/bundle.umd.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "contributors": [
    "Angular Package <angular-package@wwwdev.io>"
  ],
  "scripts": {
    "build": "./node_modules/.bin/ngc -p tsconfig.json",
    "bundle": "rollup -c rollup.config.js --environment BUILD:production",
    "bundle:dev": "rollup -c rollup.config.js --environment BUILD:development",
    "clean": "rimraf ./dist ./node_modules",
    "copyfiles:html": "cd src && copyfiles *.html **/*.html ./../dist",
    "copyfiles:scss": "cd src && copyfiles *.scss **/*.scss ./../dist",
    "copyfiles:css": "cd src && copyfiles *.css **/*.css ./../dist",
    "copyfiles:all": "npm run copyfiles:html && npm run copyfiles:scss && npm run copyfiles:css",
    "postbuild": "npm run copyfiles:all",
    "prebuild": "npm install",
    "prepare": "",
    "start": "npm run build && npm run bundle",
    "clean:start": "npm run clean && npm start",
    "test": "./node_modules/karma/bin/karma start karma.conf.js ",
    "tslint": "./node_modules/tslint/bin/tslint -p tsconfig.json --force --type-check"
  },
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@types/jasmine": "^2.8.2",
    "@types/karma": "^1.7.2",
    "@types/node": "^8.0.58",
    "codelyzer": "^4.0.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.3",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.1",
    "karma-rollup-plugin": "^0.2.4",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.5.5",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "zone.js": "^0.8.18"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angular-package/angular-package.git"
  },
  "bugs": {
    "url": "https://github.com/angular-package/angular-package/issues"
  }
}
